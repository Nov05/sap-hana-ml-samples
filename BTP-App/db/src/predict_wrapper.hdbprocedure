PROCEDURE predict_wrapper()
    LANGUAGE SQLSCRIPT
    SQL SECURITY INVOKER
    AS
BEGIN
-- dataset 
  lt_data = SELECT "STATION_UUID" as "station_uuid", "DATE" as "date", "E5"  as "e5"  FROM CPM_ML_FUEL_PRICES_RNK_TEST; 
-- model
  lt_model = SELECT * FROM CPM_ML_PAL_ADDITIVE_MODEL_ANALYSIS_MODEL;
  
  CALL base_additivemodelforecast1_predict(
    :lt_data,
    :lt_model,
    :out_0,
    :out_1,
    :out_2
    );
  truncate table CPM_ML_PAL_ADDITIVE_MODEL_PREDICT_FORECAST_RESULT_TBL_1;
  insert into  CPM_ML_PAL_ADDITIVE_MODEL_PREDICT_FORECAST_RESULT_TBL_1 SELECT * FROM :out_0;

  truncate table CPM_ML_PAL_ADDITIVE_MODEL_FORECAST_PREDICT_DECOMPOSITION_TBL_1;
  insert into  CPM_ML_PAL_ADDITIVE_MODEL_FORECAST_PREDICT_DECOMPOSITION_TBL_1 SELECT * FROM :out_1;

  truncate table CPM_ML_PAL_ADDITIVE_MODEL_FORECAST_PREDICT_ERROR_TBL_1;
  insert into  CPM_ML_PAL_ADDITIVE_MODEL_FORECAST_PREDICT_ERROR_TBL_1 SELECT * FROM :out_2;

END